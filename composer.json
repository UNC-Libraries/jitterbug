{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/UNC-Libraries/revisionable-1.git",
            "no-api": true
        }
    ],
    "require": {
        "laravel/framework": "^9.27",
        "laravelcollective/html": "^6.0",
        "solarium/solarium": "^6.2",
        "venturecraft/revisionable": "dev-main",
        "diglactic/laravel-breadcrumbs": "^6.0",
        "ramsey/uuid": "^4.0",
        "adldap2/adldap2-laravel": "^6.0",
        "jimmiw/php-time-ago": "^3.1.0",
        "laravel/tinker": "^2.7",
        "doctrine/dbal": "^2.5",
        "laravel/helpers": "^1.1",
        "laravel/ui": "^3.0",
        "fakerphp/faker": "^1.13",
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10",
        "symfony/css-selector": "^6.0",
        "symfony/dom-crawler": "^6.0",
        "laravel/homestead": "^13.2",
        "filp/whoops": "~2.0",
        "laravel/dusk": "^6.15",
        "nunomaduro/collision": "^6.3",
        "spatie/laravel-ignition": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Jitterbug\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true
    }
}
