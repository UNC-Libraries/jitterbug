ip: 192.168.10.10
memory: 2560
cpus: 1
provider: virtualbox
version: 10.1.0
authorize: ~/.ssh/id_rsa.pub
keys:
    - ~/.ssh/id_rsa
folders:
    -
        map: /placeholder/jitterbug
        to: /home/vagrant/code
sites:
    -
        map: homestead.test
        to: /home/vagrant/code/public
        php: "8.1"
databases:
    - homestead
name: jitterbug-php-8
hostname: jitterbug-php-8
features:
    - webdriver: true
